    <!-- log in -->
    <div id="login"> Log in form
        <div class="row">
            <form id="loginform" class="col s12">
                <div class="row">
                    <div class="input-field col s6">
                        <input placeholder=" Type email or User ID" id="first_name" name="email_or_id" type="text"
                            class="validate">
                        <label for="first_name">Email or User ID</label>
                    </div>

                </div>
                <div class="row">
                    <div class="input-field col s12">
                        <input name="password_or_id" id="password" type="password" class="validate">
                        <label for="password">Password</label>
                    </div>
                </div>
                <button class="btn" type="submit"> log in
                </button>

            </form>
        </div>


    </div>
    <!-- New Account-->
    <div id='new_account'>




        <div class="row">
            <form id="new_account_form" class="col s12">
                <div class="row">
                    <div class="input-field col s6">
                        <input placeholder="Scooby" id="dog_name" name="dog_name" type="text" class="validate">
                        <label for="dog_name">Dog's name</label>
                    </div>
                    <div class="input-field col s6">
                        <input placeholder="Doo" id="dog_last_name" name="dog_last_name" type="text" class="validate">
                        <label for="dog_last_name">Dog last Name</label>
                    </div>
                </div>
                <div class="row">
                    <div class="col s12 "></div>
                    <div class="input-field col s6">
                        <input placeholder="Fido" id="screen_name" name="screen_name" type="text" class="validate">
                        <label for="screen_name">Screen name</label>
                    </div>
                    <div class="input-field col m6">
                        <select id="dog_breed" name="dog_breed">
                            <option value="" selected>Dog Breed
                            </option>
                            <option value="1">Great Dane</option>
                            <option value="2">Poodle</option>
                            <option value="3">cat</option>
                        </select>
                        <label for="dog_breed">Dog Breed</label>
                    </div>
                </div>

                <div class="row">
                    <div class="input-field col s12">
                        <input id="email" name="email" type="email" class="validate">
                        <label for="email">Email</label>
                    </div>
                </div>

                <div class="row">
                    <div class="input-field col s12">
                        <input id="new_password" name="new_password" type="password" class="validate">
                        <label for="new_password">Password</label>
                    </div>
                </div>

                <button class="btn" type="submit">continue creating account
                </button>

        </div>
        </form>
    </div>
    <!-- second Stage -->
    <div id="second_stage">
        <form id="second_stage_form" class="row">
            <div class="input-field col m6 l3">
                <select id="dog_gender" name="dog_gender">
                    <option value="" selected>Dog Gender</option>
                    <option value="male">male</option>
                    <option value="Female"> Female</option>
                    <option value="Chose not to specify">Chose not to specify</option>
                </select>
                <label>Materialize Select</label>
            </div>

            <div class="input-field col m6 l3">
                <select if="fixed" name="fixed">
                    <option value="" selected>Is your dog fixed?</option>
                    <option value="1">Yes</option>
                    <option value="2">No</option>
                </select>
                <label>Materialize Select</label>
            </div>

            <div class="input-field col m6 l3">
                <select>
                    <option value="" selected>What level of education</option>
                    <option value="1">House Trained</option>
                    <option value="2">Some Tricks</option>
                    <option value="3">Graduated from a puppy academy</option>
                </select>
            </div>
            <div class="input-field col m6 l3">
                <select>
                    <option value="" selected>How does your dog interact with other dogs?
                    </option>
                    <option value="Playful">Playful</option>
                    <option value="Weiry at first but after getting to know a dog will become friendly.">Weiry at first
                        but after getting to know a dog will become friendly.</option>
                    <option value="Does not like dogs.">Does not like dogs.</option>
                </select>
                <label>Materialize Select</label>
            </div>



            <div class="row">
                <div class="row">
                    <div class="input-field col s12">
                        <textarea id="about_your_dog" name="about_your_dog" class="materialize-textarea"></textarea>
                        <label for="about_your_dog">Textarea</label>
                    </div>
                </div>
            </div>

            <div class="col s12 m6 offset-s0 offset-l3">

                <div class="card-panel">

                    <div id="inputMap"></div>
                </div>

            </div>
            <button class="btn" type="submit">Completed
            </button>
        </form>
    </div>
