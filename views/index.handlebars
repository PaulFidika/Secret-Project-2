    <!-- log in -->



    <!-- New Account-->
    <div id='new_account'>




        <div class="row">
            <form id="new_account_form" class="col s12">
                <div class="row">
                    <div class="input-field col s6">
                        <input placeholder="Scooby" id="dog_name" name="dog_name" type="text" class="validate">
                        <label for="dog_name">Dog's name</label>
                    </div>
                    <div class="input-field col s6">
                        <input placeholder="Doo" id="dog_last_name" name="dog_last_name" type="text" class="validate">
                        <label for="dog_last_name">Dog last Name</label>
                    </div>
                </div>
                <div class="row">




                    <div class="col s12 "></div>
                    <div class="input-field col s6">
                        <input placeholder="age" id="age" name="age" type="number" min="0" class="validate">
                        <label for="age">age</label>
                    </div>



                    <div class="input-field col m6">
                        <select id="breed" name="breed">
                            <option value="" selected>Dog Breed
                            </option>
                            <option value="1">Great Dane</option>
                            <option value="2">Poodle.</option>
                            <option value="3">cat.</option>
                        </select>
                        <label for="breed">Dog Breed</label>
                    </div>
                </div>

                <div class="row">
                    <div class="input-field col s12">
                        <input id="email" name="email" type="email" class="validate">
                        <label for="email">Email</label>
                    </div>
                </div>

                <div class="row">
                    <div class="input-field col s12">
                        <input id="new_password" name="new_password" type="password" class="validate">
                        <label for="new_password">Password</label>
                    </div>
                </div>



        </div>
        <div class="input-field col m6 l3">
            <select id="gender" name="gender">
                <option value="What is your dogs gender?" disabled selected>What is your dogs gender?</option>
                <option value="male">male</option>
                <option value="Female"> Female</option>
                <option value="Chose not to specify">Chose not to specify</option>
            </select>
            <label>What is your dogs gender?</label>
        </div>

        <div class="input-field col m6 l3">
            <select id="fixed" name="fixed">
                <option value="#" disabled selected>Is your dog fixed?</option>
                <option value="1">Yes</option>
                <option value="0">No</option>
            </select>
            <label>Is your dog fixed?</label>
        </div>




        <!-- second row of tbe second stage -->







        <div class="row">
            <div class="row">
                <div class="input-field col s12">
                    <textarea id="interests" name="interests" class="materialize-textarea"></textarea>
                    <label for="interests">Textarea</label>
                </div>
            </div>
        </div>


        <div class="file-field input-field">
            <div class="btn">
                <span>File</span>
                <input id="image" type="file">
            </div>
            <div class="file-path-wrapper">
                <input class="file-path validate" type="text">
            </div>
        </div>


        <div class="col s12 m6 offset-s0 offset-l3">

            <div class="card-panel">

                <div id="inputMap"></div>
            </div>

        </div>
        <button class="btn" type="submit">continue creating account
        </button>
        </form>
    </div>




</div>